---
import { LicensePricing } from '@ag-website-shared/components/license-pricing/LicensePricing';
import { fetchYouTubeThumbnail } from '@ag-website-shared/utils/fetchYoutubeThumbnail';

import Layout from '@layouts/Layout.astro';

const pricingVideoThumbnail = await fetchYouTubeThumbnail('VPr__OKxH50');
---

<Layout
    title="AG Grid: License and Pricing"
    description="View license and pricing details for AG Grid. Compare features in AG Grid Community vs. Enterprise. Purchase AG Grid Bundle for a discount Grid & Charts licenses."
    showSearchBar={true}
    showDocsNav={false}
>
    <LicensePricing client:load defaultSelection="grid" pricingVideoThumbnail={pricingVideoThumbnail} />
</Layout>
